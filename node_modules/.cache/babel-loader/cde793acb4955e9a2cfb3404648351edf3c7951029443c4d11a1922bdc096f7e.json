{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapState } from 'vuex'; //引入mapState 引入数据的辅助函数\n//这种引入方式，是将所有功能函数引入\n//import _ from 'lodash'\n//最好的引入方式是按需引入\nimport throttle from 'lodash/throttle';\nexport default {\n  name: 'TypeNav',\n  data() {\n    return {\n      //存储鼠标移上哪一个一级分类\n      currentIndex: -1\n    };\n  },\n  //组件挂载完毕向服务器发请求\n  mounted() {\n    //通知vuex发请求，获取数据，存储于仓库当中\n    this.$store.dispatch('categroyList');\n  },\n  computed: {\n    //右侧需要的是一个函数,当使用这个计算属性的时候,右侧函数会立即执行一次\n    //注入一个参数state,其实就是大仓库的数据\n    ...mapState({\n      categroyList: state => {\n        //映射为组件实例身上的数据\n        return state.home.categroyList; //大仓库的数据给到组件中的categoryList\n      }\n    })\n  },\n\n  methods: {\n    //ES5的写法  进行了节流操作\n    changeIndex: throttle(function (index) {\n      this.currentIndex = index;\n    }, 50),\n    leaveIndex() {\n      this.currentIndex = -1;\n    },\n    goSearch(event) {\n      //编程式导航+事件委派\n      //事件委派的问题：是否点的是<a></a>标签 如何获取参数（1，2，3级分类的产品名字、id）\n      let element = event.target;\n      let {\n        categroyname,\n        category1id,\n        category2id,\n        category3id\n      } = element.dataset;\n      if (categroyname) {\n        //整理路由跳转的参数\n        let location = {\n          name: 'search'\n        };\n        let query = {\n          categroyName: categroyname\n        };\n        if (category1id) {\n          query.category1Id = category1id;\n          this.$router.push();\n        } else if (category2id) {\n          query.category2Id = category2id;\n        } else {\n          query.category3Id = category3id;\n        }\n        //整理完参数\n        location.query = query;\n        //路由跳转\n        this.$router.push(location);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapState","throttle","name","data","currentIndex","mounted","$store","dispatch","computed","categroyList","state","home","methods","changeIndex","index","leaveIndex","goSearch","event","element","target","categroyname","category1id","category2id","category3id","dataset","location","query","categroyName","category1Id","$router","push","category2Id","category3Id"],"sources":["src/components/TypeNav/index.vue"],"sourcesContent":["<template>\n  <div class=\"type-nav\">\n    <div class=\"container\">\n      <!-- 事件的委派 -->\n      <div @mouseleave=\"leaveIndex()\">\n        <h2 class=\"all\">全部商品分类</h2>\n        <!-- 三级联动 -->\n        <div class=\"sort\">\n          <div class=\"all-sort-list2\" @click=\"goSearch\">\n            <!-- 一级分类 -->\n            <div\n              class=\"item\"\n              v-for=\"(c1, index) in categroyList\"\n              :key=\"c1.categoryId\"\n              :class=\"{ cur: currentIndex == index }\"\n            >\n              <h3 @mouseenter=\"changeIndex(index)\">\n                <!-- 自定义属性 -->\n                <a\n                  :data-categoryName=\"c1.categoryName\"\n                  :data-category1Id=\"c1.categoryId\"\n                  >{{ c1.categoryName }}</a\n                >\n              </h3>\n              <!-- 二级分类 -->\n              <div\n                class=\"item-list clearfix\"\n                :style=\"{ display: currentIndex == index ? 'block' : 'none' }\"\n              >\n                <div\n                  class=\"subitem\"\n                  v-for=\"(c2, index) in c1.categoryChild\"\n                  :key=\"c2.categoryId\"\n                >\n                  <dl class=\"fore\">\n                    <dt>\n                      <a\n                        :data-categoryName=\"c2.categoryName\"\n                        :data-category2Id=\"c2.categoryId\"\n                        >{{ c2.categoryName }}</a\n                      >\n                    </dt>\n                    <!-- 三级分类 -->\n                    <dd>\n                      <em\n                        v-for=\"(c3, index) in c2.categoryChild\"\n                        :key=\"c3.categoryId\"\n                      >\n                        <a\n                          :data-categoryName=\"c3.categoryName\"\n                          :data-category3Id=\"c3.categoryId\"\n                          >{{ c3.categoryName }}</a\n                        >\n                      </em>\n                    </dd>\n                  </dl>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <nav class=\"nav\">\n        <a href=\"###\">服装城</a>\n        <a href=\"###\">美妆馆</a>\n        <a href=\"###\">尚品汇超市</a>\n        <a href=\"###\">全球购</a>\n        <a href=\"###\">闪购</a>\n        <a href=\"###\">团购</a>\n        <a href=\"###\">有趣</a>\n        <a href=\"###\">秒杀</a>\n      </nav>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex' //引入mapState 引入数据的辅助函数\n//这种引入方式，是将所有功能函数引入\n//import _ from 'lodash'\n//最好的引入方式是按需引入\nimport throttle from 'lodash/throttle'\n\nexport default {\n  name: 'TypeNav',\n  data() {\n    return {\n      //存储鼠标移上哪一个一级分类\n      currentIndex: -1,\n    }\n  },\n  //组件挂载完毕向服务器发请求\n  mounted() {\n    //通知vuex发请求，获取数据，存储于仓库当中\n    this.$store.dispatch('categroyList')\n  },\n  computed: {\n    //右侧需要的是一个函数,当使用这个计算属性的时候,右侧函数会立即执行一次\n    //注入一个参数state,其实就是大仓库的数据\n    ...mapState({\n      categroyList: (state) => {\n        //映射为组件实例身上的数据\n        return state.home.categroyList //大仓库的数据给到组件中的categoryList\n      },\n    }),\n  },\n  methods: {\n    //ES5的写法  进行了节流操作\n    changeIndex: throttle(function (index) {\n      this.currentIndex = index\n    }, 50),\n    leaveIndex() {\n      this.currentIndex = -1\n    },\n    goSearch(event) {\n      //编程式导航+事件委派\n      //事件委派的问题：是否点的是<a></a>标签 如何获取参数（1，2，3级分类的产品名字、id）\n      let element = event.target\n      let { categroyname, category1id, category2id, category3id } =\n        element.dataset\n      if (categroyname) {\n        //整理路由跳转的参数\n        let location = { name: 'search' }\n        let query = { categroyName: categroyname }\n        if (category1id) {\n          query.category1Id = category1id\n          this.$router.push()\n        } else if (category2id) {\n          query.category2Id = category2id\n        } else {\n          query.category3Id = category3id\n        }\n        //整理完参数\n        location.query = query\n        //路由跳转\n        this.$router.push(location)\n      }\n    },\n  },\n}\n</script>\n\n<style lang=\"less\">\n.type-nav {\n  border-bottom: 2px solid #e1251b;\n\n  .container {\n    width: 1200px;\n    margin: 0 auto;\n    display: flex;\n    position: relative;\n\n    .all {\n      width: 210px;\n      height: 45px;\n      background-color: #e1251b;\n      line-height: 45px;\n      text-align: center;\n      color: #fff;\n      font-size: 14px;\n      font-weight: bold;\n    }\n\n    .nav {\n      a {\n        height: 45px;\n        margin: 0 22px;\n        line-height: 45px;\n        font-size: 16px;\n        color: #333;\n      }\n    }\n\n    .sort {\n      position: absolute;\n      left: 0;\n      top: 45px;\n      width: 210px;\n      height: 461px;\n      position: absolute;\n      background: #fafafa;\n      z-index: 999;\n\n      .all-sort-list2 {\n        .item {\n          h3 {\n            line-height: 30px;\n            font-size: 14px;\n            font-weight: 400;\n            overflow: hidden;\n            padding: 0 20px;\n            margin: 0;\n\n            a {\n              color: #333;\n            }\n          }\n\n          .item-list {\n            display: none;\n            position: absolute;\n            width: 734px;\n            min-height: 460px;\n            background: #f7f7f7;\n            left: 210px;\n            border: 1px solid #ddd;\n            top: 0;\n            z-index: 9999 !important;\n\n            .subitem {\n              float: left;\n              width: 650px;\n              padding: 0 4px 0 8px;\n\n              dl {\n                border-top: 1px solid #eee;\n                padding: 6px 0;\n                overflow: hidden;\n                zoom: 1;\n\n                &.fore {\n                  border-top: 0;\n                }\n\n                dt {\n                  float: left;\n                  width: 54px;\n                  line-height: 22px;\n                  text-align: right;\n                  padding: 3px 6px 0 0;\n                  font-weight: 700;\n                }\n\n                dd {\n                  float: left;\n                  width: 415px;\n                  padding: 3px 0 0;\n                  overflow: hidden;\n\n                  em {\n                    float: left;\n                    height: 14px;\n                    line-height: 14px;\n                    padding: 0 8px;\n                    margin-top: 5px;\n                    border-left: 1px solid #ccc;\n                  }\n                }\n              }\n            }\n          }\n          /* &表示嵌套上一级 */\n          /* &:hover {\n            .item-list {\n              display: block;\n            }\n          } */\n        }\n        .cur {\n          background: skyblue;\n        }\n      }\n    }\n  }\n}\n</style>\n"],"mappings":";AA8EA,SAAAA,QAAA;AACA;AACA;AACA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACA;MACAC,YAAA;IACA;EACA;EACA;EACAC,QAAA;IACA;IACA,KAAAC,MAAA,CAAAC,QAAA;EACA;EACAC,QAAA;IACA;IACA;IACA,GAAAR,QAAA;MACAS,YAAA,EAAAC,KAAA;QACA;QACA,OAAAA,KAAA,CAAAC,IAAA,CAAAF,YAAA;MACA;IACA;EACA;;EACAG,OAAA;IACA;IACAC,WAAA,EAAAZ,QAAA,WAAAa,KAAA;MACA,KAAAV,YAAA,GAAAU,KAAA;IACA;IACAC,WAAA;MACA,KAAAX,YAAA;IACA;IACAY,SAAAC,KAAA;MACA;MACA;MACA,IAAAC,OAAA,GAAAD,KAAA,CAAAE,MAAA;MACA;QAAAC,YAAA;QAAAC,WAAA;QAAAC,WAAA;QAAAC;MAAA,IACAL,OAAA,CAAAM,OAAA;MACA,IAAAJ,YAAA;QACA;QACA,IAAAK,QAAA;UAAAvB,IAAA;QAAA;QACA,IAAAwB,KAAA;UAAAC,YAAA,EAAAP;QAAA;QACA,IAAAC,WAAA;UACAK,KAAA,CAAAE,WAAA,GAAAP,WAAA;UACA,KAAAQ,OAAA,CAAAC,IAAA;QACA,WAAAR,WAAA;UACAI,KAAA,CAAAK,WAAA,GAAAT,WAAA;QACA;UACAI,KAAA,CAAAM,WAAA,GAAAT,WAAA;QACA;QACA;QACAE,QAAA,CAAAC,KAAA,GAAAA,KAAA;QACA;QACA,KAAAG,OAAA,CAAAC,IAAA,CAAAL,QAAA;MACA;IACA;EACA;AACA"},