{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SearchSelector from './SearchSelector/SearchSelector';\nimport { mapGetters, mapState } from 'vuex';\nexport default {\n  name: 'Search',\n  components: {\n    SearchSelector\n  },\n  data() {\n    return {\n      searchParams: {\n        category1Id: '',\n        category2Id: '',\n        category3Id: '',\n        categoryName: '',\n        //关键字\n        keyword: '',\n        //排序 初始状态降序\n        order: '1:desc',\n        //分页器\n        pageNo: 1,\n        //每一页展示数组的个数\n        pageSize: 10,\n        //平台售卖属性操作带的参数\n        props: [],\n        //品牌\n        trademark: ''\n      }\n    };\n  },\n  //在组件挂载之前先执行一次\n  beforeMount() {\n    //console.log(this.$route.query)\n    //复杂写法\n    //this.searchParams.category1Id=this.$route.query.category1Id\n    //简写 ES6 合并对象 Object.assign( , )\n    Object.assign(this.searchParams, this.$route.query, this.$route.params);\n  },\n  //组件挂在完毕执行一次【只执行一次】\n  mounted() {\n    //this.$store.dispatch('getSearchList', {})\n    this.getData();\n  },\n  computed: {\n    //传递数组\n    ...mapGetters(['goodsList']),\n    isOne() {\n      return this.searchParams.order.indexOf('1') != -1;\n    },\n    isTwo() {\n      return this.searchParams.order.indexOf('2') != -1;\n    },\n    isAsc() {\n      return this.searchParams.order.indexOf('asc') != -1;\n    },\n    isDesc() {\n      return this.searchParams.order.indexOf('desc') != -1;\n    },\n    //获取search魔魁啊展示产品一共多少数据\n    ...mapState({\n      total: state => state.search.searchList.total\n    })\n  },\n  methods: {\n    //发起请求 封装成函数，需要请求时直接调用\n    getData() {\n      this.$store.dispatch('getSearchList', this.searchParams);\n    },\n    removeCategoryName() {\n      this.searchParams.categoryName = undefined;\n      this.searchParams.category1Id = undefined;\n      this.searchParams.category2Id = undefined;\n      this.searchParams.category3Id = undefined;\n      this.getData();\n      //进行路由跳转\n      if (this.$route.params) {\n        this.$router.push({\n          name: 'search',\n          params: this.$route.params\n        });\n      } else {\n        this.$router.push({\n          name: 'search'\n        });\n      }\n    },\n    removeKeyword() {\n      //删除关键字\n      this.searchParams.keyword = undefined;\n      this.getData();\n      this.$bus.$emit('clear');\n      //进行路由的跳转\n      if (this.$route.params) {\n        this.$router.push({\n          name: 'search',\n          query: this.$route.query\n        });\n      } else {\n        this.$router.push({\n          name: 'search'\n        });\n      }\n    },\n    //自定义事件的回调\n    trademarkInfo(trademark) {\n      //console.log('fuzhujian', trademark)\n      this.searchParams.trademark = `${trademark.tmId}:${trademark.tmName}`;\n      this.getData();\n    },\n    removeTrademark() {\n      this.searchParams.trademark = undefined;\n      this.getData();\n    },\n    //收集平台属性\n    attrInfo(attr, attrValue) {\n      //console.log(attr, attrValue)\n      let props = `${attr.attrId}:${attrValue}:${attr.attrName}`;\n      //数组去重\n      if (this.searchParams.props.indexOf(props) == -1) {\n        this.searchParams.props.push(props);\n      }\n      //再次发起请求\n      this.getData();\n    },\n    //删除售卖属性\n    removeAttr(index) {\n      //console.log(index)\n      this.searchParams.props.splice(index, 1);\n      this.getData();\n    },\n    //排序操作\n    changeOrder(flag) {\n      //alert('123')\n      //flag形参 代表点击的是综合还是价格\n      //let originOrder = this.searchParams.order\n      let originFlag = this.searchParams.order.split(':')[0];\n      let originSort = this.searchParams.order.split(':')[1];\n      let newOrder = '';\n      if (flag == originFlag) {\n        newOrder = `${originFlag}:${originSort == 'desc' ? 'asc' : 'desc'}`;\n      } else {\n        newOrder = `${flag}:${'desc'}`;\n      }\n      //赋值\n      this.searchParams.order = newOrder;\n      this.getData();\n    },\n    getPageno(pageNo) {\n      //console.log(pageNo)\n      this.searchParams.pageNo = pageNo;\n      this.getData();\n    }\n  },\n  //监听属性值变化\n  watch: {\n    $route(newValue, oldValue) {\n      //整理参数\n      Object.assign(this.searchParams, this.$route.query, this.$route.params);\n      this.getData();\n      //每一次请求完毕 应该吧相应的1，2，3级id清空，以便下一次请求\n      this.searchParams.category1Id = undefined;\n      this.searchParams.category2Id = undefined;\n      this.searchParams.category3Id = undefined;\n    }\n  }\n};","map":{"version":3,"names":["SearchSelector","mapGetters","mapState","name","components","data","searchParams","category1Id","category2Id","category3Id","categoryName","keyword","order","pageNo","pageSize","props","trademark","beforeMount","Object","assign","$route","query","params","mounted","getData","computed","isOne","indexOf","isTwo","isAsc","isDesc","total","state","search","searchList","methods","$store","dispatch","removeCategoryName","undefined","$router","push","removeKeyword","$bus","$emit","trademarkInfo","tmId","tmName","removeTrademark","attrInfo","attr","attrValue","attrId","attrName","removeAttr","index","splice","changeOrder","flag","originFlag","split","originSort","newOrder","getPageno","watch","newValue","oldValue"],"sources":["src/pages/Search/index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <TypeNav></TypeNav>\r\n    <div class=\"main\">\r\n      <div class=\"py-container\">\r\n        <!--bread 面包屑带有x的结构-->\r\n        <div class=\"bread\">\r\n          <ul class=\"fl sui-breadcrumb\">\r\n            <li>\r\n              <a href=\"#\">全部结果</a>\r\n            </li>\r\n          </ul>\r\n          <ul class=\"fl sui-tag\">\r\n            <!-- 分类的面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.categoryName\">\r\n              {{ searchParams.categoryName\r\n              }}<i @click=\"removeCategoryName\">×</i>\r\n            </li>\r\n            <!-- 关键字的面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.keyword\">\r\n              {{ searchParams.keyword }}<i @click=\"removeKeyword\">×</i>\r\n            </li>\r\n            <!-- 品牌信息的面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.trademark\">\r\n              {{ searchParams.trademark.split(':')[1]\r\n              }}<i @click=\"removeTrademark\">×</i>\r\n            </li>\r\n            <!-- 平台售卖属性的面包屑 -->\r\n            <li\r\n              class=\"with-x\"\r\n              v-for=\"(attrValue, index) in searchParams.props\"\r\n              :key=\"index\"\r\n            >\r\n              {{ attrValue.split(':')[1] }}<i @click=\"removeAttr(index)\">×</i>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n\r\n        <!--selector-->\r\n        <SearchSelector @trademarkInfo=\"trademarkInfo\" @attrInfo=\"attrInfo\" />\r\n\r\n        <!--details-->\r\n        <div class=\"details clearfix\">\r\n          <div class=\"sui-navbar\">\r\n            <div class=\"navbar-inner filter\">\r\n              <ul class=\"sui-nav\">\r\n                <li :class=\"{ active: isOne }\" @click=\"changeOrder(1)\">\r\n                  <a\r\n                    >综合<span\r\n                      v-show=\"isOne\"\r\n                      class=\"iconfont\"\r\n                      :class=\"{ 'icon-up': isAsc, 'icon-Down-': isDesc }\"\r\n                    ></span\r\n                  ></a>\r\n                </li>\r\n                <li :class=\"{ active: isTwo }\" @click=\"changeOrder(2)\">\r\n                  <a\r\n                    >价格<span\r\n                      v-show=\"isTwo\"\r\n                      class=\"iconfont\"\r\n                      :class=\"{ 'icon-up': isAsc, 'icon-Down-': isDesc }\"\r\n                    ></span\r\n                  ></a>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n          <!-- 销售产品列表 -->\r\n          <div class=\"goods-list\">\r\n            <ul class=\"yui3-g\">\r\n              <li\r\n                class=\"yui3-u-1-5\"\r\n                v-for=\"(good, index) in goodsList\"\r\n                :key=\"good.id\"\r\n              >\r\n                <div class=\"list-wrap\">\r\n                  <div class=\"p-img\">\r\n                    <router-link><img :src=\"good.defaultImg\" /></router-link>\r\n                  </div>\r\n                  <div class=\"price\">\r\n                    <strong>\r\n                      <em>¥</em>\r\n                      <i>{{ good.price }}</i>\r\n                    </strong>\r\n                  </div>\r\n                  <div class=\"attr\">\r\n                    <a\r\n                      target=\"_blank\"\r\n                      href=\"item.html\"\r\n                      title=\"促销信息，下单即赠送三个月CIBN视频会员卡！【小米电视新品4A 58 火爆预约中】\"\r\n                      >{{ good.title }}</a\r\n                    >\r\n                  </div>\r\n                  <div class=\"commit\">\r\n                    <i class=\"command\">已有<span>2000</span>人评价</i>\r\n                  </div>\r\n                  <div class=\"operate\">\r\n                    <a\r\n                      href=\"success-cart.html\"\r\n                      target=\"_blank\"\r\n                      class=\"sui-btn btn-bordered btn-danger\"\r\n                      >加入购物车</a\r\n                    >\r\n                    <a href=\"javascript:void(0);\" class=\"sui-btn btn-bordered\"\r\n                      >收藏</a\r\n                    >\r\n                  </div>\r\n                </div>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <!-- 分页器 -->\r\n          <!-- 测试阶段使用假数据 -->\r\n          <Pagination\r\n            :pageNo=\"searchParams.pageNo\"\r\n            :pageSize=\"searchParams.pageSize\"\r\n            :total=\"total\"\r\n            :continues=\"5\"\r\n            @getPageno=\"getPageno\"\r\n          ></Pagination>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SearchSelector from './SearchSelector/SearchSelector'\r\nimport { mapGetters, mapState } from 'vuex'\r\nexport default {\r\n  name: 'Search',\r\n  components: {\r\n    SearchSelector,\r\n  },\r\n  data() {\r\n    return {\r\n      searchParams: {\r\n        category1Id: '',\r\n        category2Id: '',\r\n        category3Id: '',\r\n        categoryName: '',\r\n        //关键字\r\n        keyword: '',\r\n        //排序 初始状态降序\r\n        order: '1:desc',\r\n        //分页器\r\n        pageNo: 1,\r\n        //每一页展示数组的个数\r\n        pageSize: 10,\r\n        //平台售卖属性操作带的参数\r\n        props: [],\r\n        //品牌\r\n        trademark: '',\r\n      },\r\n    }\r\n  },\r\n  //在组件挂载之前先执行一次\r\n  beforeMount() {\r\n    //console.log(this.$route.query)\r\n    //复杂写法\r\n    //this.searchParams.category1Id=this.$route.query.category1Id\r\n    //简写 ES6 合并对象 Object.assign( , )\r\n    Object.assign(this.searchParams, this.$route.query, this.$route.params)\r\n  },\r\n  //组件挂在完毕执行一次【只执行一次】\r\n  mounted() {\r\n    //this.$store.dispatch('getSearchList', {})\r\n    this.getData()\r\n  },\r\n\r\n  computed: {\r\n    //传递数组\r\n    ...mapGetters(['goodsList']),\r\n    isOne() {\r\n      return this.searchParams.order.indexOf('1') != -1\r\n    },\r\n    isTwo() {\r\n      return this.searchParams.order.indexOf('2') != -1\r\n    },\r\n    isAsc() {\r\n      return this.searchParams.order.indexOf('asc') != -1\r\n    },\r\n    isDesc() {\r\n      return this.searchParams.order.indexOf('desc') != -1\r\n    },\r\n    //获取search魔魁啊展示产品一共多少数据\r\n    ...mapState({\r\n      total: (state) => state.search.searchList.total,\r\n    }),\r\n  },\r\n  methods: {\r\n    //发起请求 封装成函数，需要请求时直接调用\r\n    getData() {\r\n      this.$store.dispatch('getSearchList', this.searchParams)\r\n    },\r\n    removeCategoryName() {\r\n      this.searchParams.categoryName = undefined\r\n      this.searchParams.category1Id = undefined\r\n      this.searchParams.category2Id = undefined\r\n      this.searchParams.category3Id = undefined\r\n      this.getData()\r\n      //进行路由跳转\r\n      if (this.$route.params) {\r\n        this.$router.push({ name: 'search', params: this.$route.params })\r\n      } else {\r\n        this.$router.push({ name: 'search' })\r\n      }\r\n    },\r\n    removeKeyword() {\r\n      //删除关键字\r\n      this.searchParams.keyword = undefined\r\n      this.getData()\r\n      this.$bus.$emit('clear')\r\n      //进行路由的跳转\r\n      if (this.$route.params) {\r\n        this.$router.push({ name: 'search', query: this.$route.query })\r\n      } else {\r\n        this.$router.push({ name: 'search' })\r\n      }\r\n    },\r\n    //自定义事件的回调\r\n    trademarkInfo(trademark) {\r\n      //console.log('fuzhujian', trademark)\r\n      this.searchParams.trademark = `${trademark.tmId}:${trademark.tmName}`\r\n      this.getData()\r\n    },\r\n    removeTrademark() {\r\n      this.searchParams.trademark = undefined\r\n      this.getData()\r\n    },\r\n    //收集平台属性\r\n    attrInfo(attr, attrValue) {\r\n      //console.log(attr, attrValue)\r\n      let props = `${attr.attrId}:${attrValue}:${attr.attrName}`\r\n      //数组去重\r\n      if (this.searchParams.props.indexOf(props) == -1) {\r\n        this.searchParams.props.push(props)\r\n      }\r\n      //再次发起请求\r\n      this.getData()\r\n    },\r\n    //删除售卖属性\r\n    removeAttr(index) {\r\n      //console.log(index)\r\n      this.searchParams.props.splice(index, 1)\r\n      this.getData()\r\n    },\r\n    //排序操作\r\n    changeOrder(flag) {\r\n      //alert('123')\r\n      //flag形参 代表点击的是综合还是价格\r\n      //let originOrder = this.searchParams.order\r\n      let originFlag = this.searchParams.order.split(':')[0]\r\n      let originSort = this.searchParams.order.split(':')[1]\r\n      let newOrder = ''\r\n      if (flag == originFlag) {\r\n        newOrder = `${originFlag}:${originSort == 'desc' ? 'asc' : 'desc'}`\r\n      } else {\r\n        newOrder = `${flag}:${'desc'}`\r\n      }\r\n      //赋值\r\n      this.searchParams.order = newOrder\r\n      this.getData()\r\n    },\r\n    getPageno(pageNo) {\r\n      //console.log(pageNo)\r\n      this.searchParams.pageNo = pageNo\r\n      this.getData()\r\n    },\r\n  },\r\n  //监听属性值变化\r\n  watch: {\r\n    $route(newValue, oldValue) {\r\n      //整理参数\r\n      Object.assign(this.searchParams, this.$route.query, this.$route.params)\r\n      this.getData()\r\n      //每一次请求完毕 应该吧相应的1，2，3级id清空，以便下一次请求\r\n      this.searchParams.category1Id = undefined\r\n      this.searchParams.category2Id = undefined\r\n      this.searchParams.category3Id = undefined\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.main {\r\n  margin: 10px 0;\r\n\r\n  .py-container {\r\n    width: 1200px;\r\n    margin: 0 auto;\r\n\r\n    .bread {\r\n      margin-bottom: 5px;\r\n      overflow: hidden;\r\n\r\n      .sui-breadcrumb {\r\n        padding: 3px 15px;\r\n        margin: 0;\r\n        font-weight: 400;\r\n        border-radius: 3px;\r\n        float: left;\r\n\r\n        li {\r\n          display: inline-block;\r\n          line-height: 18px;\r\n\r\n          a {\r\n            color: #666;\r\n            text-decoration: none;\r\n\r\n            &:hover {\r\n              color: #4cb9fc;\r\n            }\r\n          }\r\n        }\r\n      }\r\n\r\n      .sui-tag {\r\n        margin-top: -5px;\r\n        list-style: none;\r\n        font-size: 0;\r\n        line-height: 0;\r\n        padding: 5px 0 0;\r\n        margin-bottom: 18px;\r\n        float: left;\r\n\r\n        .with-x {\r\n          font-size: 12px;\r\n          margin: 0 5px 5px 0;\r\n          display: inline-block;\r\n          overflow: hidden;\r\n          color: #000;\r\n          background: #f7f7f7;\r\n          padding: 0 7px;\r\n          height: 20px;\r\n          line-height: 20px;\r\n          border: 1px solid #dedede;\r\n          white-space: nowrap;\r\n          transition: color 400ms;\r\n          cursor: pointer;\r\n\r\n          i {\r\n            margin-left: 10px;\r\n            cursor: pointer;\r\n            font: 400 14px tahoma;\r\n            display: inline-block;\r\n            height: 100%;\r\n            vertical-align: middle;\r\n          }\r\n\r\n          &:hover {\r\n            color: #28a3ef;\r\n          }\r\n        }\r\n      }\r\n    }\r\n\r\n    .details {\r\n      margin-bottom: 5px;\r\n\r\n      .sui-navbar {\r\n        overflow: visible;\r\n        margin-bottom: 0;\r\n\r\n        .filter {\r\n          min-height: 40px;\r\n          padding-right: 20px;\r\n          background: #fbfbfb;\r\n          border: 1px solid #e2e2e2;\r\n          padding-left: 0;\r\n          border-radius: 0;\r\n          box-shadow: 0 1px 4px rgba(0, 0, 0, 0.065);\r\n\r\n          .sui-nav {\r\n            position: relative;\r\n            left: 0;\r\n            display: block;\r\n            float: left;\r\n            margin: 0 10px 0 0;\r\n\r\n            li {\r\n              float: left;\r\n              line-height: 18px;\r\n\r\n              a {\r\n                display: block;\r\n                cursor: pointer;\r\n                padding: 11px 15px;\r\n                color: #777;\r\n                text-decoration: none;\r\n              }\r\n\r\n              &.active {\r\n                a {\r\n                  background: #e1251b;\r\n                  color: #fff;\r\n                }\r\n              }\r\n            }\r\n          }\r\n        }\r\n      }\r\n\r\n      .goods-list {\r\n        margin: 20px 0;\r\n\r\n        ul {\r\n          display: flex;\r\n          flex-wrap: wrap;\r\n\r\n          li {\r\n            height: 100%;\r\n            width: 20%;\r\n            margin-top: 10px;\r\n            line-height: 28px;\r\n\r\n            .list-wrap {\r\n              .p-img {\r\n                padding-left: 15px;\r\n                width: 215px;\r\n                height: 255px;\r\n\r\n                a {\r\n                  color: #666;\r\n\r\n                  img {\r\n                    max-width: 100%;\r\n                    height: auto;\r\n                    vertical-align: middle